---
const { url } = Astro.props.node
const correctedUrl = url.replace('https://', '')
const newUrl = `https://w.soundcloud.com/player/?url=https%3A//${correctedUrl}`
---

<div class='content-soundcloud'>
  <iframe
    width='100%'
    height='166'
    scrolling='no'
    frameborder='no'
    allow='autoplay'
    src={newUrl}></iframe>
</div>
